{
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "endOfLine": "auto",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "dotenv/config",
    "@/orpc/server",
    "react",
    "next",
    "next/*",
    "^@/(.*)$",
    "^[./]",
    "^@/(.css)$"
  ],
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-packagejson",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
